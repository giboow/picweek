{% extends "PicweekMainBundle::layout.html.twig" %}

{% block content %}

    <table border=1>
    {% for data in paginator.currentPageResults %}
        <tr>
            <td><a href="{{ path('_place_get',{ "id": data.id }) }}">{{ data.getName() }}</a></td>
            <td>{{ data.getAddress() }}</td>
            <td>{{ data.getLongitude() }}</td>
            <td>{{ data.getLatitude() }}</td>
        </tr>
    {% endfor %}
    </table>
    {{ pagerfanta(paginator, 'default', pagerfanta_opts) }}

{% endblock %}
